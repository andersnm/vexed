{
  "name": "vexed-syntax",
  "displayName": "Vexed DSL Syntax",
  "version": "0.0.1",
  "publisher": "andersnm",
  "engines": {
    "vscode": "*"
  },
  "scripts": { 
    "build": "vsce package"
  },
  "contributes": {
    "languages": [
      {
        "id": "vexed",
        "aliases": [
          "Vexed DSL"
        ],
        "extensions": [
          ".vexed"
        ]
      }
    ],
    "grammars": [
      {
        "language": "vexed",
        "scopeName": "source.vexed",
        "path": "./syntaxes/vexed.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^3.7.1"
  }
}
