class Main() {
   testComplex(i: int, flag: bool): int {
     let x: int[] = [ 1, 2, 3 ];
     let y: int = x[i];
     
     if (flag) {
       let z: int[] = [ 4, 5, 6 ];
       return z[0];
     } else {
       return y;
     }
   }
   
   testNestedArray(): int {
     let matrix: int[][] = [ [ 1, 2 ], [ 3, 4 ] ];
     return matrix[0][1];
   }
   
   public output1: int = this.testComplex(0, false);
   public output2: int = this.testNestedArray();
}
