class Base(strValue: string, intValue: int, boolValue: bool) {
  public baseStr: string = strValue;
  public baseNum: int = intValue;
  public baseBool: bool = boolValue;
}

class Sidecar(baseObj: Base) {
  public sidecarStr: string = baseObj.baseStr;
  public sidecarNum: int = baseObj.baseNum;
  public sidecarBool: bool = baseObj.baseBool;
}

class Main() extends Base("String, it is", 321, true) {
  public mainStr: string = "It's a string";
  public mainNum: int = 123;
  public mainBool: bool = true;
  public mainBase: Base = Base(this.mainStr, this.mainNum, this.mainBool);
  public mainSidecar: Sidecar = Sidecar(this.mainBase);
}
