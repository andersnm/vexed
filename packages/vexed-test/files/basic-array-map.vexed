class ExtStuff() {
  public proof: string = "Member";
  public literally: string[] = [ "1", "2", "3" ].map(this.canWeDoIt);

  string canWeDoIt(str: string) {
    return "Yes we can: " + str + ": " + this.proof;
  }
}

class Main() {
  public strArray: string[] = [ "ABC", "DEFGH" ];
  public temp: string = "TEMP";

  public ext: ExtStuff = ExtStuff();

  public wrapped: string[] = this.strArray.map(this.wrapString);
  public lengths: int[] = this.strArray.map(this.getLength);
  public canReally: string[] = this.strArray.map(this.ext.canWeDoIt);

  string wrapString(s: string) {
    return "[" + s + "]" + this.temp;
  }

  int getLength(s: string) {
    return s.length;
  }

}
